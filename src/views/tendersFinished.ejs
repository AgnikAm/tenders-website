<% title = 'Zakończone przetargi' %>

<h1 class="mb-4">Zakończone przetargi</h1>

<table class="table table-bordered table-striped">
  <thead class="table-dark">
    <tr>
      <th>#</th>
      <th>Tytuł</th>
      <th>Status</th>
      <th>Data zakończenia przyjmowania ofert</th>
    </tr>
  </thead>
  <tbody>
    <% tenders.forEach((tender, index) => { %>
      <tr>
        <td><%= index + 1 %></td>
        <td><a href="/finished-tenders/<%= tender.id %>"><%= tender.title %></a></td>
        <td>Zakończony</td>
        <td><%= new Date(tender.endDate).toLocaleString('pl-PL') %></td>
      </tr>
    <% }) %>
  </tbody>
</table>
